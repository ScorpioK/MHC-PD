{
	"title": "GitBook",
	"description": "GitBook",
	"author": "zhangjikai",
	"language": "zh-hans",
	"gitbook": "3.2.3",
	"root": ".",
	"plugins": [
		"-lunr",
		"-search",
		"-highlight",
		"-livereload",
		"search-plus@^0.0.11",
		"simple-page-toc@^0.1.1",
		"edit-link@^2.0.2",
		"disqus@^0.1.0",
		"advanced-emoji@^0.2.1",
		"anchors@^0.7.1",
		"include-codeblock@^3.0.2",
		"ace@^0.3.2",
		"emphasize@^1.1.0",
		"katex@^1.1.3",
		"splitter@^0.0.8",
		"mermaid-gb3@2.1.0",
		"tbfed-pagefooter@^0.0.1",
		"expandable-chapters-small@^0.1.7",
		"sectionx@^3.1.0",
		"donate@^1.0.2",
		"local-video@^1.0.1",
		"sitemap-general@^0.1.1",
		"anchor-navigation-ex@0.1.8",
		"favicon@^0.0.2",
		"todo@^0.1.3",
		"terminal@^0.3.2",
		"alerts@^0.2.0",
		"include-csv@^0.1.0",
		"puml@^1.0.1",
		"musicxml@^1.0.2",
		"klipse@^1.2.0",
		"rss@^3.0.2",
		"-sharing",
		"sharing-plus@^0.0.2",
		"graph@^0.1.0",
		"chart@^0.2.0"
	],

	"pluginsConfig": {
		"theme-default": {
			"showLevel": true
		},
		"disqus": {
			"shortName": "评论"
		},
		"include-codeblock": {
			"template": "ace",
			"unindent": true,
			"edit": true
		},

		"tbfed-pagefooter": {
			"copyright": "Mallcoo",
			"modify_label": "",
			"modify_format": "YYYY-MM-DD HH:mm:ss"
		},
		"donate": {
			"wechat": "https://zhangjikai.com/resource/weixin.png",
			"alipay": "https://zhangjikai.com/resource/alipay.png",
			"title": "",
			"button": "",
			"alipayText": "",
			"wechatText": ""
		},
		"simple-page-toc": {
			"maxDepth": 3,
			"skipFirstH1": true
		},
		"edit-link": {
			"base": "https://github.com/zhangjikai/gitbook-use/edit/master",
			"label": "Edit This Page"
		},
		"sitemap-general": {
			"prefix": "http://gitbook.zhangjikai.com"
		},
		"anchor-navigation-ex": {
			"isRewritePageTitle": false,
			"tocLevel1Icon": "fa fa-hand-o-right",
			"tocLevel2Icon": "fa fa-hand-o-right",
			"tocLevel3Icon": "fa fa-hand-o-right"
		},
		"sectionx": {
			"tag": "b"
		},

		"terminal": {
			"copyButtons": true,
			"fade": false,
			"style": "flat"
		},

		"rss": {
			"title": "GitBook",
			"description": "GitBook",
			"author": "Jikai Zhang",
			"feed_url": "http://gitbook.zhangjikai.com/rss",
			"site_url": "http://gitbook.zhangjikai.com/",
			"managingEditor": "me@zhangjikai.com",
			"webMaster": "me@zhangjikai.com",
			"categories": [
				"gitbook"
			]
		}
	}
}